{
  "manifest_version": 3,
  "name": "Tab Wrangler",
  "version": "1.0",
  "description": "A browser extension that classifies and prioritizes your open tabs.",
  "permissions": ["tabs", "activeTab"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "background": {
    "scripts": ["background.js"]
  }
} 